    },
    "devDependencies": {
        "@biomejs/biome": "2.2.2",
        "@cloudflare/vite-plugin": "^1.2.3",
        "vite": "^6.3.5",
        "vite-ssr-components": "^0.5.1",
        "wrangler": "^4.17.0"
    }
}
